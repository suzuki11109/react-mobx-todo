{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": false,
    "declaration": false,
    "removeComments": true,
    "experimentalDecorators": true,
    "noLib": false,
    "jsx": "react",
    "outDir": "dist",
    "sourceMap": true
  },
  "awesomeTypescriptLoaderOptions": {
    "forkChecker": true,
    "useCache": true
  },
  "exclude": [
    "node_modules",
    "static"
  ]
}
